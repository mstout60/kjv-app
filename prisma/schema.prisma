generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model kjv {
  id        Int    @id
  testament String
  book      String
  chapter   String
  verse     String
  vtext     String
  vscript   String
  hashed    String
}

model Books {
  id String @id @default(uuid())
  testament String
  name String
}